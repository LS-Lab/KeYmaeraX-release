    diall;
    diffInvariant({`v = old(v) - b()*t`}, 1);
    diffInvariant({`beta = old(beta) + t/r*(v + b()/2*t)`}, 1);
    diffInvariant({`-t * (v + b()/2*t) <= x - old(x) & x - old(x) <= t * (v + b()/2*t)`}, 1);
